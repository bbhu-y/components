{
  "name": "edt-formsy",
  "version": "0.2.6",
  "description": "使用formsy包装的表单组件",
  "main": "index.js",
  "scripts": {
    "build": "npm run test && cross-env NODE_ENV=production gulp build",
    "pub": "npm run build && cd build && npm publish",
    "_dvp": "gulp copy && cd ./build && npm link && gulp watchCopy",
    "dvp": "gulp build && cd ./build && npm link && cd ../ && gulp watchCompile",
    "lint": "eslint --ext .js --ext .jsx components",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js",
    "site": "cross-env ENV=production bisheng build -c ./site/bisheng.config.js",
    "start": "antd-tools run clean && rimraf _site && mkdir _site && cross-env NODE_ENV=development bisheng start -c ./site/bisheng.config.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.nie.netease.com:32200/edt-frontend/edt-formsy.git"
  },
  "author": "Weber <huangweiduo@corp.mail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "antd": "^3.26.18",
    "antd-tools": "^7.7.1",
    "babel-jest": "^26.3.0",
    "babel-plugin-import": "^1.13.0",
    "bisheng": "^1.6.1",
    "bisheng-plugin-antd": "^1.3.2",
    "bisheng-plugin-description": "^0.1.4",
    "bisheng-plugin-react": "^1.1.1",
    "bisheng-plugin-toc": "^0.4.4",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.6.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "humps": "^2.0.1",
    "intersection-observer": "^0.11.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "edt-common": "^1.0.3",
    "formsy-react": "^1.1.4",
    "lodash": "^4.17.20",
    "lz-string": "^1.4.4",
    "moment": "^2.27.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-intl": "^5.9.4",
    "scrollama": "^2.2.1"
  }
